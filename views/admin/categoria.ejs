<%- include('header') %>
         
<h2>
    Admin Categorias:
    <a href="/admin/categoria/nova">Nova categoria</a>
</h2> 
<div>
    <ul>
        <% categoria.forEach( categoria => { %> 
        <li>
            <%- categoria.categoria %> |
            <a href="/admin/categoria/delete/<%- categoria.id%>">Excluir</a> 
            <a href="/admin/categoria/editar/<%- categoria.id%>">Editar</a>
        </li>
        <% }) %>
    </ul>
</div>

<%- include('footer') %>
